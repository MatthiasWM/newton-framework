{//	Name References	IsNameRef: func(item)	IsInstance(item, 'nameRef),	AliasFromObj: func(item)		begin		if not item then			return;		if IsInstance(item, 'alias) then			item		else if IsNameRef(item) then			item._alias		else if IsSoupEntry(item) then			MakeEntryAlias(item)		end,	EntryFromObj: func(item)		begin		if not item then			return;		if IsInstance(item, 'alias) then			ResolveEntryAlias(item)		else if IsFrame(item) and IsEntryAlias(item._alias) then			ResolveEntryAlias(item._alias)		else if IsSoupEntry(item) then			item		end,	ObjEntryClass: func(item)		begin		if not item then			return;		if IsNameRef(item) then			item._entryClass		else if not ClassOf(item) = 'frame and IsFrame(item) then			ClassOf(item)		else			begin			local entry := EntryFromObj(item);			if entry then				begin				if entry.class then					entry.class				else if entry.viewStationery then					entry.viewStationery				else if entry.sorton then					begin					if ClassOf(entry.sorton) = 'name then						'person					else						'company					end				else					ClassOf(entry)				end			end		end,}