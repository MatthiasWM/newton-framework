/*	File:		ErrorNotify.h	Contains:	Error notification declarations.	Written by:	Newton Research Group, 2007.*/#if !defined(__ERRORNOTIFY_H)#define __ERRORNOTIFY_H 1//	Alert notification levelsenum NotifyLevel{	kNotifyLog = 1,	kNotifyMessage,	kNotifyAlert,	kNotifyQAlert,	kModalAlert,	kModalQAlert};/*------------------------------------------------------------------------------	F u n c t i o n   P r o t o t y p e s------------------------------------------------------------------------------*/void		ErrorNotify(NewtonErr inErr, NotifyLevel inErrLevel);void		ActionErrorNotify(NewtonErr inErr, NotifyLevel inErrLevel);void		BadWickedNaughtyNoot(int inErr);#endif	/* __ERRORNOTIFY_H */